{
  "config": {
    "mode": "failover",
    "storage": false,
    "snowflake": false,
    "azure": true,
    "domain": "Sales",
    "environment": "qa"
  },
  "batchAccountScale": {
    "scaleUp": {
      "resourceGroup": "DRSalesRG",
      "batch": "DRBatchSales",
      "pool": "DRpoolBatchSales"
    },
    "scaleDown": {
      "resourceGroup": "qaSalesRG",
      "batch": "qaBatchSales",
      "pool": "qapoolBatchSales"
    }
  },
  "ADFTrigger": {
    "start": {
      "resourceGroup": "DRSalesRG",
      "adf": "DRSalesADF"
    },
    "stop": {
      "resourceGroup": "qaSalesRG",
      "adf": "qaSalesADF"
    }
  },
  "kvSync": {
    "from": {
      "resourceGroup": "qaSalesRG",
      "kv": "qaKvSales"
    },
    "to": {
      "resourceGroup": "DRSalesRG",
      "kv": "DRKvSales"
    }
  },
  "StgSync": {
    "from": {
      "resourceGroup": "qaSalesRG",
      "stg": "qaStorageSales"
    },
    "to": {
      "resourceGroup": "DRSalesRG",
      "stg": "DRStorageSales"
    }
  }
}